# SentinelHub API Gateway Environment Variables

# Server Configuration
PORT=5000
WS_PORT=8080
NODE_ENV=development

# Clerk Authentication
CLERK_PUBLISHABLE_KEY=pk_test_your_publishable_key_here
CLERK_SECRET_KEY=sk_test_your_secret_key_here

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=sentinelhub123

# GitHub Integration (for non-Clerk GitHub users)
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# External Security APIs
VIRUSTOTAL_API_KEY=your_virustotal_api_key
SHODAN_API_KEY=your_shodan_api_key
ABUSEIPDB_API_KEY=your_abuseipdb_api_key

# AWS Configuration (for S3 scanning)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1

# AI Integration
OPENAI_API_KEY=your_openai_api_key_optional
GEMMA_API_ENDPOINT=http://localhost:8000/generate

# Monitoring
GRAFANA_API_KEY=your_grafana_api_key
PROMETHEUS_ENDPOINT=http://localhost:9090

# Security Scanner Configurations
SONARQUBE_URL=http://localhost:9000
SONARQUBE_TOKEN=your_sonarqube_token